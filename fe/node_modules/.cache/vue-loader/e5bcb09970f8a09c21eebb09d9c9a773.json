nst path = `http://localhost:5000/getMetrics/${this.purpose}`;\n      axios\n        .get(path)\n        .then((res) => {\n          // eslint-disable-next-line\n          //Ccnsole.error(res);\n          this.metrics = res.data;\n          if (Object.entries(res.data).length > 0) {\n            this.disabled = false;\n          } else {\n            this.disabled = true;\n          }\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.error(error);\n        });\n    },\n  },\n  mounted() {\n    this.bus.$on('submit', this.getMessage);\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped></style>\n"]}]}